<h2>List of ratings</h2>


<ul>
 <% @ratings.each do |rating| %>
   <li> 
        <%= rating %> 
        <%= button_to 'Delete', rating_path(rating), method: :delete, form: { data: { turbo_confirm: 'Confirm deletion' } } %>
    </li>
 <% end %>
</ul>

<p>
  <%= link_to "New rating", new_rating_path %>
</p>


